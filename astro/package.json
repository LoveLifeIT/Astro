{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "TAILWIND_MODE=watch astro dev --watch",
    "start": "astro dev --watch --host 0.0.0.0",
    "build": "astro check && astro build",
    "start:server": "astro dev",
    "build:server": "astro check && astro build --config astro.config.server.mjs",
    "build:static": "astro check && astro build --config astro.config.static.mjs",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/node": "^8.2.5",
    "@astrojs/sitemap": "^3.1.4",
    "astro": "^4.8.6",
    "astro-seo": "^0.8.3",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  }
}
